#include<iostream>
using namespace std;
class node{
    public:
    int data ;
    node* next ;

    node(int data){
        this -> data = data ;
        next = NULL ;
    }
};
node *takeInput(){
    node *head = NULL ;
    node *tail =NULL ;
    int data ;
    cin >> data ;
    while(data != -1){
         node *newNode = new node(data) ;
        if(head == NULL){
            head = newNode ;
            tail = newNode ;
        }
        else{
            tail -> next = newNode ;
            tail = newNode  ;// or we can write tail = tail -> next
             }
        cin >> data ;
    }
    return head ;
}
void print(node *head){
    node * temp = head ;
    while(temp != NULL){
        cout << temp -> data <<" ";
        temp = temp -> next ;
    }
}
int length(Node *head)
{
    int len=0;
    while(head!=NULL)
    {
        head=head->next;
        len++;
    }
    return len;
 }
int main(){
    node *head = takeInput() ;
    print(head) ;
    cout << length(head);   // to find the length of the linked list
}
